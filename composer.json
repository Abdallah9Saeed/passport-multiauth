{
    "name": "hamedov/passport-multiauth",
    "description": "Multiauth and custom grants for laravel passport",
    "require": {
        "laravel/passport": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Hamed",
            "email": "m.hamed@nezam.io"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Hamedov\\PassportMultiauth\\Providers\\PassportServiceProvider",
                "Hamedov\\PassportMultiauth\\Providers\\MultiauthServiceProvider"
            ],
            "dont-discover": [
                "laravel/passport"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Hamedov\\PassportMultiauth\\": "src/"
        }
    }
}
