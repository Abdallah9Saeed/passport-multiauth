{
    "name": "hamedov/passport-multiauth",
    "description": "Multiauth and custom grants for laravel passport",
    "keywords": ["laravel", "passport", "multiauth", "custom grant"],
    "require": {
        "php": "^7.2",
        "laravel/passport": "^8.0"
    },
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hamedov93/passport-multiauth/issues",
        "source": "https://github.com/hamedov93/passport-multiauth"
    },
    "authors": [
        {
            "name": "Mohamed Hamed",
            "email": "m.hamed@nezam.io"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hamedov\\PassportMultiauth\\Providers\\PassportServiceProvider",
                "Hamedov\\PassportMultiauth\\Providers\\MultiauthServiceProvider"
            ],
            "dont-discover": [
                "laravel/passport"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Hamedov\\PassportMultiauth\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
